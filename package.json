{
  "name": "kaggle-notebook-automation",
  "version": "1.0.0",
  "type": "module",
  "description": "Chrome extension for automating Kaggle notebooks with LangGraph and OpenAI",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "vite build && node copy-files.js",
    "type-check": "tsc --noEmit",
    "postbuild": "node copy-files.js"
  },
  "dependencies": {
    "@langchain/core": "^0.2.0",
    "@langchain/langgraph": "^0.0.30",
    "@langchain/openai": "^0.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "lucide-react": "^0.400.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  }
}
